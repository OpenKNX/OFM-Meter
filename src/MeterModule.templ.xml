                    <choose ParamRefId="%AID%_UP-%TT%%CC%001_R-%TT%%CC%00101">
                      <!-- Aktiv -->
                      <when test="!=0">

                        <!-- Sperre -->
                        <choose ParamRefId="%AID%_UP-%TT%%CC%001_R-%TT%%CC%00101">
                          <!-- bei Standard, Betriebsstunden-->
                          <when test="1 3">
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%002_R-%TT%%CC%00201" Icon="lock-outline" />
                            <choose ParamRefId="%AID%_UP-%TT%%CC%002_R-%TT%%CC%00201">
                              <when test="1"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%003_R-%TT%%CC%00301" /></when>
                              <when test="2"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%003_R-%TT%%CC%00302" /></when>
                            </choose>
                          </when>
                        </choose>

                        <ParameterSeparator Id="%AID%_PS-nnn" Text="" UIHint="HorizontalRuler" />
                        
                        <!-- Eingang -->
                        <ParameterSeparator Id="%AID%_PS-nnn" Text="Eingang" UIHint="Headline" />                        
                        <choose ParamRefId="%AID%_UP-%TT%%CC%001_R-%TT%%CC%00101">
                          <!-- bei Standard -->
                          <when test="1">
                            <!-- Typ -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%010_R-%TT%%CC%01001" IndentLevel="1" />
                            <choose ParamRefId="%AID%_UP-%TT%%CC%010_R-%TT%%CC%01001">
                              <!-- DPT 12-->
                              <when test="0"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%001_R-%TT%%CC%00101" /></when>
                              <!-- DPT 13-->
                              <when test="1"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%001_R-%TT%%CC%00102" /></when>
                              <!-- DPT 14-->
                              <when test="2"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%001_R-%TT%%CC%00103" /></when>
                            </choose>

                            <!-- Multiplizieren -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%011_R-%TT%%CC%01101" IndentLevel="1" />
                            <!-- Rücklaufsperre -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%018_R-%TT%%CC%01801" IndentLevel="1" />
                            <!-- Ignoriere 0 -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%017_R-%TT%%CC%01701" IndentLevel="1" />
                            <!-- Max Abstand -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%019_R-%TT%%CC%01901" IndentLevel="1" />
                          </when>

                          <!-- bei Impuls -->
                          <when test="2">
                            <ComObjectRefRef RefId="%AID%_O-%TT%%CC%001_R-%TT%%CC%00105" />
                            <!-- je Einheit -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%016_R-%TT%%CC%01601" IndentLevel="1" />
                            <!-- Leistung -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%027_R-%TT%%CC%02701" IndentLevel="1" />
                          </when>

                          <!-- bei Betriebsstunden -->
                          <when test="3">
                            <ComObjectRefRef RefId="%AID%_O-%TT%%CC%001_R-%TT%%CC%00104" />
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%015_R-%TT%%CC%01501" IndentLevel="1" />
                          </when>
                        </choose>
                        
                        <ParameterSeparator Id="%AID%_PS-nnn" Text="" UIHint="HorizontalRuler" />
                        <ParameterSeparator Id="%AID%_PS-nnn" Text="Ausgang" UIHint="Headline" />

                        <choose ParamRefId="%AID%_UP-%TT%%CC%001_R-%TT%%CC%00101">
                          <!-- bei Standard, Impulse -->
                          <when test="1 2">
                            <!-- Ausgang -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%020_R-%TT%%CC%02001" IndentLevel="1" />
                            <choose ParamRefId="%AID%_UP-%TT%%CC%020_R-%TT%%CC%02001">
                              <!-- DPT 12-->
                              <when test="0"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00201" /></when>
                              <!-- DPT 13-->
                              <when test="1"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00202" /></when>
                            </choose>

                            <!-- Multiplizieren -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%021_R-%TT%%CC%02101" IndentLevel="1" />

                          </when>
                          <!-- bei Betriebsstunden -->
                          <when test="3">
                            <!-- Ausgang -->
                            <ParameterRefRef RefId="%AID%_UP-%TT%%CC%029_R-%TT%%CC%02901" IndentLevel="1" />
                            <choose ParamRefId="%AID%_UP-%TT%%CC%029_R-%TT%%CC%02901">
                              <when test="0"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00210" /></when>
                              <when test="1"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00211" /></when>
                              <when test="2"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00212" /></when>
                              <when test="3"><ComObjectRefRef RefId="%AID%_O-%TT%%CC%002_R-%TT%%CC%00213" /></when>
                            </choose>
                          </when>
                        </choose>

                        <!-- Reset-->
                        <ComObjectRefRef RefId="%AID%_O-%TT%%CC%004_R-%TT%%CC%00401" />
                      </when>
                    </choose>



